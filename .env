# Enhanced .env file for React frontend with debugging

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:8181/api/api

# Optional: ML API if you have separate ML services
REACT_APP_ML_API_BASE_URL=http://localhost:8081/ml

# Weather API (if used directly from frontend)
REACT_APP_WEATHER_API_KEY=your_weather_api_key_here

# Application Settings
REACT_APP_APP_NAME=Maize Admin Portal
REACT_APP_VERSION=1.0.0

# Development Settings - ENABLE THESE FOR DEBUGGING
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Enable detailed API logging (set to false in production)
REACT_APP_ENABLE_API_LOGGING=true

# Network timeout settings (in milliseconds)
REACT_APP_API_TIMEOUT=30000

# Security Settings (for development only)
REACT_APP_TOKEN_STORAGE_KEY=maize_auth_token
REACT_APP_USER_STORAGE_KEY=maize_user_data

# Error Reporting
REACT_APP_ENABLE_ERROR_REPORTING=true

# Mock Data Settings (for development/testing)
REACT_APP_USE_MOCK_DATA_ON_ERROR=true

# Backend Health Check Interval (in ms)
REACT_APP_HEALTH_CHECK_INTERVAL=300000